{
  "name": "@ghost/cache",
  "version": "1.5.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./stores": {
      "import": "./dist/stores.mjs",
      "require": "./dist/stores.js",
      "types": "./dist/stores.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js",
      "types": "./dist/middleware.d.ts"
    }
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["ghost", "cache", "cloudflare", "tiered", "swr", "memory", "latency"],
  "bugs": {
    "url": "https://github.com/getghost/ghost/issues"
  },
  "homepage": "https://github.com/getghost/ghost#readme",
  "files": ["./dist/**"],
  "author": "Developers <andreas@ghost.dev>",
  "scripts": {
    "build": "tsup",
    "test": "vitest run"
  },
  "devDependencies": {
    "@libsql/client": "^0.7.0",
    "@types/node": "^20.14.9",
    "@upstash/redis": "^1.0.0",
    "next": "^14.2.10",
    "tsup": "^8.0.2",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@opentelemetry/api": "1.8.0",
    "@ghost/error": "workspace:^",
    "superjson": "^2.2.1"
  }
}
